<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homegym">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PushUps"
                android:label="Push Ups"/>
        <activity android:name=".Plank"
                android:label="Plank"/>
        <activity android:name=".CobraStrech"
                android:label="Cobra Strech"/>
        <activity android:name=".AbdominalCrunches"
                android:label="Abdominal Crunches"/>
        <activity android:name=".JumpingJack"
                android:label="Jumping Jack"/>
        <activity android:name=".HOME_WORKOUT"
                android:label="HOME WORKOUT"
                android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.homegym.LoginActivity"/>
        </activity>
        <activity android:name=".Signup"
            android:label="Signup"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.homegym.LoginActivity" />
        </activity>
        <activity android:name=".LoginActivity"
            android:label="Login"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>